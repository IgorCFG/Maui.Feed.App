<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Style TargetType="NavigationPage">
        <Setter Property="BarBackgroundColor" Value="{StaticResource White}" />
        <Setter Property="BarTextColor" Value="{StaticResource Secondary}" />
    </Style>

    <Style x:Key="MainPageStyle" TargetType="TabbedPage">
        <Setter Property="UnselectedTabColor" Value="{StaticResource Quartiary}" />
        <Setter Property="SelectedTabColor" Value="{StaticResource Secondary}" />
    </Style>

</ResourceDictionary>
